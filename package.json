    "@ngrx/store": "13.2.0",
    "@ngrx/store-devtools": "13.2.0",
    "@nrwl/angular": "14.3.1",
    "@types/crc": "^3.4.0",
    "crc": "^3.8.0",
    "git": "^0.1.5",
    "ngx-toastr": "^14.1.3",
    "reflect-metadata": "^0.1.13",
