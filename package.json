    "@nestjs/websockets": "^8.0.11",
    "@ngrx/effects": "^12.4.0",
    "@ngrx/store": "^12.4.0",
    "@ngrx/store-devtools": "^12.4.0",
    "@nrwl/angular": "12.9.0",
    "@types/crc": "^3.4.0",
    "crc": "^3.8.0",
    "git": "^0.1.5",
    "ngx-toastr": "^14.1.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.6.0",
