{
  "name": "boosted-revolt-tool",
  "version": "0.2.0",
  "author": "Michael Castiau",
  "license": "proprietary",
  "description": "Boosted ReVOLT! Firmware conf iguration and update tool",
  "repository": "https://github.com/MichaelCastiau/boosted-revolt-tool.git",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main && exitzero electron-builder install-app-deps",
    "start": "nx serve",
    "start:app": "nx run electron:serve",
    "build": "nx build",
    "test": "nx test",
    "rebuild": "electron-rebuild -f",
    "package:mac": "dotenv -e .env -- yarn build && nx run electron:make --platform=mac",
    "release:mac": "dotenv -e .env -- yarn build && nx run electron:make --platform=mac --publishPolicy=always",
    "package:win": "dotenv -e .env -- yarn build && nx run electron:make --platform=windows",
    "release:win": "dotenv -e .env -- yarn build && nx run electron:make --platform=windows --publishPolicy=always"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "14.0.1",
